algoritmo "contagem"
var
   contador, numero: inteiro
inicio
    Escreval("Digite o at√© que numero deve ir a sequencia: ")
    Leia(numero)
    Escreval("=============================================")
   Contador <- 0
   enquanto (contador <= numero) e (contador >= 0) faca
      Escreval(contador)
      contador <- contador +1
   fimenquanto
   Escreval("Terminei a Contagem")

